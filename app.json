{
  "expo": {
    "name": "MACrossover",
    "slug": "macrossover",
    "scheme": "macrossover",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.macrossover.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.macrossover.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/favicon.ico"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-splash-screen",
      "expo-notifications",
      "expo-router"
    ],
    "extra": {
      "webviewUrl": "https://macrossover.com",
      "eas": {
        "projectId": "ad78a513-57d9-420f-b1ae-10b521d00f62"
      }
    }
  }
}
